{
  "expo": {
    "name": "lgpgapp",
    "slug": "lgpgapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconLgpd.png",
    "splash": {
      "image": "./assets/splashLgpd.png",
      "resizeMode": "contain",
      "backgroundColor": "#ECFAFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ECFAFF"
      }
    },
    "web": {
      "favicon": "./assets/lgpdFavicon.png"
    }
  }
}
